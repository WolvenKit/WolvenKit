<reactiveUi:ReactiveUserControl
    x:TypeArguments="tools:AssetBrowserViewModel"
    x:Class="WolvenKit.Views.Tools.AssetBrowserView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:reactiveUi="http://reactiveui.net"
    xmlns:converters1="clr-namespace:WolvenKit.Functionality.Converters"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:hc="https://handyorg.github.io/handycontrol"
    xmlns:iconPacks="http://metro.mahapps.com/winfx/xaml/iconpacks"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:svgc="http://sharpvectors.codeplex.com/svgc/"
    xmlns:syncfusion="http://schemas.syncfusion.com/wpf"
    xmlns:local="clr-namespace:WolvenKit"
    xmlns:I="clr-namespace:System.Windows.Interactivity;assembly=System.Windows.Interactivity"
    xmlns:tools="clr-namespace:WolvenKit.ViewModels.Tools;assembly=WolvenKit.App"
    d:DataContext="{d:DesignInstance Type=tools:AssetBrowserViewModel}"
    BorderBrush="{x:Null}"
    mc:Ignorable="d">


    <Grid>

        <Border
            hc:ThemeManager.RequestedAccentColor="{DynamicResource MahApps.Brushes.Accent3}"
            BorderBrush="{DynamicResource MahApps.Brushes.Accent4}"
            BorderThickness="0">
            <Grid>

                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto"/>
                    <RowDefinition Height="*" />
                </Grid.RowDefinitions>

                <!-- Asset Browser Search -->
                <Grid Grid.Row="0" Margin="3" >

                    <Grid.Resources>
                        <ResourceDictionary>
                            <ResourceDictionary.MergedDictionaries>
                                <hc:ThemeResources />
                                <hc:Theme />
                            </ResourceDictionary.MergedDictionaries>
                        </ResourceDictionary>
                    </Grid.Resources>

                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="*"/>
                    </Grid.ColumnDefinitions>

                    <hc:SearchBar Grid.Column="0"
                                  x:Name="FileSearchBar"
                                  Style="{StaticResource SearchBarPlus}"
                                  VerticalAlignment="Top"
                                  FlowDirection="LeftToRight"
                                  ShowClearButton="False"
                                  hc:InfoElement.Placeholder="(e.g. judy kind:mesh,ent)"
                                  hc:InfoElement.Title="Search all game files: "
                                  hc:InfoElement.TitlePlacement="Left"
                                  SearchStarted="FileSearchBar_SearchStarted"
                                  >
                    </hc:SearchBar>

                    <Viewbox Grid.Column="1"  Margin="-35,0,0,0" Height="30" Width="30" >
                        <ToggleButton IsChecked="{Binding IsRegexSearchEnabled}" >

                            <ToggleButton.Style>
                                <Style TargetType="ToggleButton" BasedOn="{StaticResource {x:Type ToggleButton}}">
                                    <Style.Triggers>
                                        <Trigger Property="IsChecked" Value="True">
                                            <Setter Property="Foreground" Value="{DynamicResource MahApps.Brushes.Accent}" />
                                        </Trigger>
                                    </Style.Triggers>
                                </Style>
                            </ToggleButton.Style>

                            <iconPacks:PackIconCodicons Kind="Regex" />
                            
                        </ToggleButton>
                    </Viewbox>
                    
                    <hc:SearchBar  Grid.Column="2" Margin="10,0,0,0"
                                  x:Name="OptionsSearchBar"
                                  Style="{StaticResource SearchBarPlus}"
                                  VerticalAlignment="Top"
                                  FlowDirection="LeftToRight"
                                  ShowClearButton="True"
                                  hc:InfoElement.Placeholder="(e.g. *.ent, base/**/judy)"
                                  hc:InfoElement.Title="files to include: "
                                  hc:InfoElement.TitlePlacement="Left"
                                  SearchStarted="FileSearchBar_SearchStarted"
                                  >
                       
                    </hc:SearchBar>

                </Grid>
                
                <!-- Asset Browser  -->
                <Grid Grid.Row="1">
                    
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="Auto" MinWidth="200"/>
                        <ColumnDefinition Width="5" />
                        <ColumnDefinition Width="*" />
                    </Grid.ColumnDefinitions>

                    <!-- Left Navigation -->
                    <Grid Grid.Column="0" Margin="2">

                        <Grid.Resources>
                            <converters1:ExtensionToImageConverter x:Key="ExtensionToImageConverter" />
                        </Grid.Resources>

                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="*"/>
                        </Grid.RowDefinitions>

                        <!-- Search Bar -->
                        <Grid Grid.Row="0" Margin="2">

                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="Auto"/>
                            </Grid.ColumnDefinitions>
                            
                            <hc:SearchBar
                                x:Name="FolderSearchBar"
                                Margin="0,0,5,0"
                                VerticalAlignment="Top"
                                Background="{DynamicResource ContentBackgroundAlt}"
                                BorderBrush="{DynamicResource BorderBrush}"
                                FlowDirection="LeftToRight"
                                hc:InfoElement.Placeholder="Search"
                                Style="{StaticResource SearchBarPlus}"
                                hc:InfoElement.Necessary="True"
                                SearchStarted="FolderSearchBar_OnSearchStarted"
                                ShowClearButton="True"
                               />

                            <Button Grid.Column="1"
                                HorizontalAlignment="Right"
                                VerticalAlignment="Center"
                                Background="{DynamicResource ContentBackground}"
                                Click="LeftNavigationHomeButton_OnClick"
                                Visibility="Visible">
                                <iconPacks:PackIconCodicons Kind="Home" />
                            </Button>
                        </Grid>

                        <!--Folder Tree -->
                        <syncfusion:SfTreeGrid Grid.Row="1" Margin="2"
                                x:Name="LeftNavigation"
                                AllowFiltering="False"
                                AllowSorting="False"
                                AllowTriStateSorting="False"
                                AutoExpandMode="RootNodesExpanded"
                                AutoGenerateColumns="False"
                                ChildPropertyName="Directories"
                                ColumnSizer="FillColumn"
                                FilterLevel="Extended"
                                SelectionChanged="LeftNavigation_OnSelectionChanged"
                                ExpandStateMappingName="IsExpanded"
                            >

                            <syncfusion:SfTreeGrid.Resources>
                                <Style TargetType="syncfusion:TreeGridCell">
                                    <Setter Property="BorderBrush" Value="Transparent" />
                                    <Setter Property="BorderThickness" Value="0" />
                                </Style>
                                <Style TargetType="syncfusion:TreeGridExpanderCell">
                                    <Setter Property="BorderBrush" Value="Transparent" />
                                    <Setter Property="BorderThickness" Value="0" />
                                </Style>
                                <converters1:ExtensionToBitmapConverter x:Key="ExtensionToBmp" />
                            </syncfusion:SfTreeGrid.Resources>

                            <syncfusion:SfTreeGrid.Columns>

                                <syncfusion:TreeGridTextColumn
                                        
                                        HeaderText="Name"
                                        HorizontalHeaderContentAlignment="Left"
                                        ImmediateUpdateColumnFilter="True"
                                        MappingName="Name">
                                    
                                    <syncfusion:TreeGridTextColumn.CellTemplate>
                                        <DataTemplate>
                                            <Grid>
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="20" />
                                                    <ColumnDefinition Width="*" />
                                                </Grid.ColumnDefinitions>
                                                <Image Height="13">
                                                    <Image.Style>
                                                        <Style TargetType="Image">
                                                            <Style.Triggers>
                                                                <DataTrigger Binding="{Binding Extension}" Value="">
                                                                    <Setter Property="Source" Value="{svgc:SvgImage /Resources/Media/Images/Icons/Files/default.svg}" />
                                                                </DataTrigger>
                                                                <DataTrigger Binding="{Binding Extension}" Value="OpenDirImageKey">
                                                                    <Setter Property="Source" Value="{svgc:SvgImage /Resources/Media/Images/Icons/Files/folderopened.svg}" />
                                                                </DataTrigger>
                                                                <DataTrigger Binding="{Binding Extension}" Value="ClosedDirImageKey">
                                                                    <Setter Property="Source" Value="{svgc:SvgImage /Resources/Media/Images/Icons/Files/folderdefault.svg}" />
                                                                </DataTrigger>
                                                                <DataTrigger Binding="{Binding Extension}" Value="RawImageKey">
                                                                    <Setter Property="Source" Value="{svgc:SvgImage /Resources/Media/Images/Icons/Files/folderclosed.svg}" />
                                                                </DataTrigger>
                                                                <DataTrigger Binding="{Binding Extension}" Value="RawModImageKey">
                                                                    <Setter Property="Source" Value="{svgc:SvgImage /Resources/Media/Images/Icons/Files/folderclosed.svg}" />
                                                                </DataTrigger>
                                                                <DataTrigger Binding="{Binding Extension}" Value="RawDlcImageKey">
                                                                    <Setter Property="Source" Value="{svgc:SvgImage /Resources/Media/Images/Icons/Files/folderclosed.svg}" />
                                                                </DataTrigger>

                                                                <DataTrigger Binding="{Binding Extension}" Value="RadishImageKey">
                                                                    <Setter Property="Source" Value="{svgc:SvgImage /Resources/Media/Images/Icons/Files/folderclosed.svg}" />
                                                                </DataTrigger>
                                                                <DataTrigger Binding="{Binding Extension}" Value="ModImageKey">
                                                                    <Setter Property="Source" Value="{svgc:SvgImage /Resources/Media/Images/Icons/Files/folderclosed.svg}" />
                                                                </DataTrigger>
                                                                <DataTrigger Binding="{Binding Extension}" Value="ModCookedImageKey">
                                                                    <Setter Property="Source" Value="{svgc:SvgImage /Resources/Media/Images/Icons/Files/folderclosed.svg}" />
                                                                </DataTrigger>
                                                                <DataTrigger Binding="{Binding Extension}" Value="ModUncookedImageKey">
                                                                    <Setter Property="Source" Value="{svgc:SvgImage /Resources/Media/Images/Icons/Files/folderclosed.svg}" />
                                                                </DataTrigger>
                                                                <DataTrigger Binding="{Binding Extension}" Value="DlcImageKey">
                                                                    <Setter Property="Source" Value="{svgc:SvgImage /Resources/Media/Images/Icons/Files/folderclosed.svg}" />
                                                                </DataTrigger>
                                                                <DataTrigger Binding="{Binding Extension}" Value="DlcCookedImageKey">
                                                                    <Setter Property="Source" Value="{svgc:SvgImage /Resources/Media/Images/Icons/Files/folderclosed.svg}" />
                                                                </DataTrigger>
                                                                <DataTrigger Binding="{Binding Extension}" Value="DlcUncookedImageKey">
                                                                    <Setter Property="Source" Value="{svgc:SvgImage /Resources/Media/Images/Icons/Files/folderclosed.svg}" />
                                                                </DataTrigger>
                                                            </Style.Triggers>
                                                        </Style>
                                                    </Image.Style>
                                                </Image>
                                                <TextBlock
                                                        x:Name="TemplateTextBlock"
                                                        Grid.Column="1"
                                                        Margin="2"
                                                        VerticalAlignment="Center"
                                                        Text="{Binding Path=Name}" />
                                            </Grid>
                                        </DataTemplate>
                                    </syncfusion:TreeGridTextColumn.CellTemplate>
                                    
                                </syncfusion:TreeGridTextColumn>
                            </syncfusion:SfTreeGrid.Columns>

                            <syncfusion:SfTreeGrid.ExpanderContextMenu>
                                <ContextMenu>
                                    <MenuItem Click="Expand_OnClick" Header="Expand" />
                                    <MenuItem Click="ExpandAll_OnClick" Header="Expand all" />
                                    <MenuItem Click="Collapse_OnClick" Header="Collapse" />
                                    <MenuItem Click="CollapseAll_OnClick" Header="Collapse all" />
                                </ContextMenu>
                            </syncfusion:SfTreeGrid.ExpanderContextMenu>

                        </syncfusion:SfTreeGrid>

                    </Grid>

                    <syncfusion:SfGridSplitter Grid.Column="1" HorizontalAlignment="Stretch" />

                    <!-- Right File List -->
                    <Grid Grid.Column="2" Margin="4">
                            
                            <Grid.RowDefinitions>
                                <RowDefinition Height="*" />
                                <RowDefinition Height="Auto" />
                            </Grid.RowDefinitions>

                            <syncfusion:SfDataGrid
                                x:Name="InnerList"
                                AllowDeleting="False"
                                AllowDraggingColumns="False"
                                AllowDraggingRows="False"
                                AllowEditing="False"
                                AllowFiltering="True"
                                AllowResizingColumns="True"
                                AllowSorting="True"
                                AutoGenerateColumns="False"
                                Background="{DynamicResource RegionBrush}"
                                ColumnSizer="Auto"
                                EnableDataVirtualization="True"
                                IsManipulationEnabled="False"
                                SelectionChanged="InnerList_SelectionChanged"
                                SelectionMode="Extended"
                                MouseDoubleClick="InnerList_MouseDoubleClick"
                                >

                                <syncfusion:SfDataGrid.RecordContextMenu>
                                    <ContextMenu>
                                        <MenuItem Click="RightContextMenuAddAll_OnClick" Header="Add selected items to Project" />
                                        <MenuItem Header="BK2">
                                            <MenuItem
                                                x:Name="VidPreviewMenuItem"
                                                Click="VidPreviewMenuItem_Click"
                                                Header="Preview BK2" />
                                            <MenuItem
                                                x:Name="BKExport"
                                                Click="BKExport_Click"
                                                Header="Export BK2 to Avi"
                                                Visibility="Collapsed"/>
                                        </MenuItem>
                                    </ContextMenu>
                                </syncfusion:SfDataGrid.RecordContextMenu>

                                <syncfusion:SfDataGrid.Resources>
                                    
                                    <Style TargetType="syncfusion:GridHeaderCellControl">
                                        <Setter Property="BorderThickness" Value="0,0,0,1" />
                                        <Setter Property="BorderBrush" Value="{DynamicResource SecondaryRegionBrush}" />
                                    </Style>
                                    <Style TargetType="syncfusion:GridCell">
                                        <Setter Property="BorderThickness" Value="0,0,0,0" />
                                    </Style>

                                    <converters1:ExtensionToBitmapConverter x:Key="ExtensionToBmp" />

                                <DataTemplate x:Key="TemplateToolTip">
                                    <TextBlock Text="{Binding FullName}" />
                                </DataTemplate>

                            </syncfusion:SfDataGrid.Resources>

                                <syncfusion:SfDataGrid.Columns>

                                    <syncfusion:GridCheckBoxColumn
                                        AllowEditing="True"
                                        HeaderText="#"
                                        MappingName="IsChecked">
                                        <syncfusion:GridCheckBoxColumn.HeaderTemplate>
                                            <DataTemplate>
                                            <CheckBox
                                                Command="local:SFDataGridCommands.CheckAndUnCheck"
                                                CommandParameter="{Binding ElementName=InnerList}"
                                                />
                                            </DataTemplate>
                                        </syncfusion:GridCheckBoxColumn.HeaderTemplate>
                                    </syncfusion:GridCheckBoxColumn>

                                    <syncfusion:GridTemplateColumn
                                        Width="22"
                                        AllowFiltering="False"
                                        MappingName=".">
                                        <syncfusion:GridTemplateColumn.CellTemplate>

                                            <DataTemplate>
                                                <Image Width="17" Margin="5,0,0,0">
                                                    <Image.Style>
                                                        <Style TargetType="Image">
                                                            <Style.Triggers>
                                                                <DataTrigger Binding="{Binding Extension}" Value="w2cent">
                                                                    <Setter Property="Source" Value="{svgc:SvgImage /Resources/Media/Images/Icons/Files/entity.svg}" />
                                                                </DataTrigger>

                                                                <DataTrigger Binding="{Binding Extension}" Value="w2phase">
                                                                    <Setter Property="Source" Value="{svgc:SvgImage /Resources/Media/Images/Icons/Files/quest_phase.svg}" />
                                                                </DataTrigger>

                                                                <DataTrigger Binding="{Binding Extension}" Value="w2beh">
                                                                    <Setter Property="Source" Value="{svgc:SvgImage /Resources/Media/Images/Icons/Files/behavior_graph3.svg}" />
                                                                </DataTrigger>

                                                                <DataTrigger Binding="{Binding Extension}" Value="w2behtree ">
                                                                    <Setter Property="Source" Value="{svgc:SvgImage /Resources/Media/Images/Icons/Files/behavior_graph3.svg}" />
                                                                </DataTrigger>

                                                                <DataTrigger Binding="{Binding Extension}" Value="env">
                                                                    <Setter Property="Source" Value="{svgc:SvgImage /Resources/Media/Images/Icons/Files/env.svg}" />
                                                                </DataTrigger>

                                                                <DataTrigger Binding="{Binding Extension}" Value="gidata">
                                                                    <Setter Property="Source" Value="{svgc:SvgImage /Resources/Media/Images/Icons/Files/gidata.svg}" />
                                                                </DataTrigger>

                                                                <DataTrigger Binding="{Binding Extension}" Value="gradient">
                                                                    <Setter Property="Source" Value="{svgc:SvgImage /Resources/Media/Images/Icons/Files/gradient.svg}" />
                                                                </DataTrigger>

                                                                <DataTrigger Binding="{Binding Extension}" Value="lipmap">
                                                                    <Setter Property="Source" Value="{svgc:SvgImage /Resources/Media/Images/Icons/Files/lipmap.svg}" />
                                                                </DataTrigger>

                                                                <DataTrigger Binding="{Binding Extension}" Value="mlmask">
                                                                    <Setter Property="Source" Value="{svgc:SvgImage /Resources/Media/Images/Icons/Files/mlmask.svg}" />
                                                                </DataTrigger>

                                                                <DataTrigger Binding="{Binding Extension}" Value="mltemplate">
                                                                    <Setter Property="Source" Value="{svgc:SvgImage /Resources/Media/Images/Icons/Files/mltemplate.svg}" />
                                                                </DataTrigger>

                                                                <DataTrigger Binding="{Binding Extension}" Value="w2animev">
                                                                    <Setter Property="Source" Value="{svgc:SvgImage /Resources/Media/Images/Icons/Files/w2animev.svg}" />
                                                                </DataTrigger>
                                                                <DataTrigger Binding="{Binding Extension}" Value="">
                                                                    <Setter Property="Source" Value="{svgc:SvgImage /Resources/Media/Images/Icons/Files/default.svg}" />
                                                                </DataTrigger>
                                                                <DataTrigger Binding="{Binding Extension}" Value="OpenDirImageKey">
                                                                    <Setter Property="Source" Value="{svgc:SvgImage /Resources/Media/Images/Icons/Files/folderopened.svg}" />
                                                                </DataTrigger>
                                                                <DataTrigger Binding="{Binding Extension}" Value="ClosedDirImageKey">
                                                                    <Setter Property="Source" Value="{svgc:SvgImage /Resources/Media/Images/Icons/Files/folderdefault.svg}" />
                                                                </DataTrigger>
                                                                <DataTrigger Binding="{Binding Extension}" Value="RawImageKey">
                                                                    <Setter Property="Source" Value="{svgc:SvgImage /Resources/Media/Images/Icons/Files/folderclosed.svg}" />
                                                                </DataTrigger>
                                                                <DataTrigger Binding="{Binding Extension}" Value="RawModImageKey">
                                                                    <Setter Property="Source" Value="{svgc:SvgImage /Resources/Media/Images/Icons/Files/folderclosed.svg}" />
                                                                </DataTrigger>
                                                                <DataTrigger Binding="{Binding Extension}" Value="RawDlcImageKey">
                                                                    <Setter Property="Source" Value="{svgc:SvgImage /Resources/Media/Images/Icons/Files/folderclosed.svg}" />
                                                                </DataTrigger>

                                                                <DataTrigger Binding="{Binding Extension}" Value="RadishImageKey">
                                                                    <Setter Property="Source" Value="{svgc:SvgImage /Resources/Media/Images/Icons/Files/folderclosed.svg}" />
                                                                </DataTrigger>
                                                                <DataTrigger Binding="{Binding Extension}" Value="ModImageKey">
                                                                    <Setter Property="Source" Value="{svgc:SvgImage /Resources/Media/Images/Icons/Files/folderclosed.svg}" />
                                                                </DataTrigger>
                                                                <DataTrigger Binding="{Binding Extension}" Value="ModCookedImageKey">
                                                                    <Setter Property="Source" Value="{svgc:SvgImage /Resources/Media/Images/Icons/Files/folderclosed.svg}" />
                                                                </DataTrigger>
                                                                <DataTrigger Binding="{Binding Extension}" Value="ModUncookedImageKey">
                                                                    <Setter Property="Source" Value="{svgc:SvgImage /Resources/Media/Images/Icons/Files/folderclosed.svg}" />
                                                                </DataTrigger>
                                                                <DataTrigger Binding="{Binding Extension}" Value="DlcImageKey">
                                                                    <Setter Property="Source" Value="{svgc:SvgImage /Resources/Media/Images/Icons/Files/folderclosed.svg}" />
                                                                </DataTrigger>
                                                                <DataTrigger Binding="{Binding Extension}" Value="DlcCookedImageKey">
                                                                    <Setter Property="Source" Value="{svgc:SvgImage /Resources/Media/Images/Icons/Files/folderclosed.svg}" />
                                                                </DataTrigger>
                                                                <DataTrigger Binding="{Binding Extension}" Value="DlcUncookedImageKey">
                                                                    <Setter Property="Source" Value="{svgc:SvgImage /Resources/Media/Images/Icons/Files/folderclosed.svg}" />
                                                                </DataTrigger>

                                                                <DataTrigger Binding="{Binding Extension}" Value="aiarch">
                                                                    <Setter Property="Source" Value="{svgc:SvgImage /Resources/Media/Images/Icons/Files/aiarch.svg}" />
                                                                </DataTrigger>
                                                                <DataTrigger Binding="{Binding Extension}" Value="animgraph">
                                                                    <Setter Property="Source" Value="{svgc:SvgImage /Resources/Media/Images/Icons/Files/animgraph.svg}" />
                                                                </DataTrigger>
                                                                <DataTrigger Binding="{Binding Extension}" Value="anims">
                                                                    <Setter Property="Source" Value="{svgc:SvgImage /Resources/Media/Images/Icons/Files/anims.svg}" />
                                                                </DataTrigger>
                                                                <DataTrigger Binding="{Binding Extension}" Value="apb">
                                                                    <Setter Property="Source" Value="{svgc:SvgImage /Resources/Media/Images/Icons/Files/apb.svg}" />
                                                                </DataTrigger>
                                                                <DataTrigger Binding="{Binding Extension}" Value="archetypes">
                                                                    <Setter Property="Source" Value="{svgc:SvgImage /Resources/Media/Images/Icons/Files/archetypes.svg}" />
                                                                </DataTrigger>
                                                                <DataTrigger Binding="{Binding Extension}" Value="areas">
                                                                    <Setter Property="Source" Value="{svgc:SvgImage /Resources/Media/Images/Icons/Files/areas.svg}" />
                                                                </DataTrigger>
                                                                <DataTrigger Binding="{Binding Extension}" Value="audio_metadata">
                                                                    <Setter Property="Source" Value="{svgc:SvgImage /Resources/Media/Images/Icons/Files/audio_metadata.svg}" />
                                                                </DataTrigger>
                                                                <DataTrigger Binding="{Binding Extension}" Value="audiovehcurveset">
                                                                    <Setter Property="Source" Value="{svgc:SvgImage /Resources/Media/Images/Icons/Files/audiovehcurveset.svg}" />
                                                                </DataTrigger>
                                                                <DataTrigger Binding="{Binding Extension}" Value="behavior">
                                                                    <Setter Property="Source" Value="{svgc:SvgImage /Resources/Media/Images/Icons/Files/behavior.svg}" />
                                                                </DataTrigger>
                                                                <DataTrigger Binding="{Binding Extension}" Value="bikecurveset">
                                                                    <Setter Property="Source" Value="{svgc:SvgImage /Resources/Media/Images/Icons/Files/bikecurveset.svg}" />
                                                                </DataTrigger>
                                                                <DataTrigger Binding="{Binding Extension}" Value="bk2">
                                                                    <Setter Property="Source" Value="{svgc:SvgImage /Resources/Media/Images/Icons/Files/bk2.svg}" />
                                                                </DataTrigger>
                                                                <DataTrigger Binding="{Binding Extension}" Value="blend">
                                                                    <Setter Property="Source" Value="{svgc:SvgImage /Resources/Media/Images/Icons/Files/blend.svg}" />
                                                                </DataTrigger>
                                                                <DataTrigger Binding="{Binding Extension}" Value="bnk">
                                                                    <Setter Property="Source" Value="{svgc:SvgImage /Resources/Media/Images/Icons/Files/bnk.svg}" />
                                                                </DataTrigger>
                                                                <DataTrigger Binding="{Binding Extension}" Value="buffer">
                                                                    <Setter Property="Source" Value="{svgc:SvgImage /Resources/Media/Images/Icons/Files/buffer.svg}" />
                                                                </DataTrigger>
                                                                <DataTrigger Binding="{Binding Extension}" Value="camcurveset">
                                                                    <Setter Property="Source" Value="{svgc:SvgImage /Resources/Media/Images/Icons/Files/camcurveset.svg}" />
                                                                </DataTrigger>
                                                                <DataTrigger Binding="{Binding Extension}" Value="cfoliage">
                                                                    <Setter Property="Source" Value="{svgc:SvgImage /Resources/Media/Images/Icons/Files/cfoliage.svg}" />
                                                                </DataTrigger>
                                                                <DataTrigger Binding="{Binding Extension}" Value="cminimap">
                                                                    <Setter Property="Source" Value="{svgc:SvgImage /Resources/Media/Images/Icons/Files/cminimap.svg}" />
                                                                </DataTrigger>
                                                                <DataTrigger Binding="{Binding Extension}" Value="community">
                                                                    <Setter Property="Source" Value="{svgc:SvgImage /Resources/Media/Images/Icons/Files/community.svg}" />
                                                                </DataTrigger>
                                                                <DataTrigger Binding="{Binding Extension}" Value="conversations">
                                                                    <Setter Property="Source" Value="{svgc:SvgImage /Resources/Media/Images/Icons/Files/conversations.svg}" />
                                                                </DataTrigger>
                                                                <DataTrigger Binding="{Binding Extension}" Value="cooked_mlsetup">
                                                                    <Setter Property="Source" Value="{svgc:SvgImage /Resources/Media/Images/Icons/Files/cooked_mlsetup.svg}" />
                                                                </DataTrigger>
                                                                <DataTrigger Binding="{Binding Extension}" Value="cookedanims">
                                                                    <Setter Property="Source" Value="{svgc:SvgImage /Resources/Media/Images/Icons/Files/cookedanims.svg}" />
                                                                </DataTrigger>
                                                                <DataTrigger Binding="{Binding Extension}" Value="cookedapp">
                                                                    <Setter Property="Source" Value="{svgc:SvgImage /Resources/Media/Images/Icons/Files/cookedapp.svg}" />
                                                                </DataTrigger>
                                                                <DataTrigger Binding="{Binding Extension}" Value="credits">
                                                                    <Setter Property="Source" Value="{svgc:SvgImage /Resources/Media/Images/Icons/Files/credits.svg}" />
                                                                </DataTrigger>
                                                                <DataTrigger Binding="{Binding Extension}" Value="cubemap">
                                                                    <Setter Property="Source" Value="{svgc:SvgImage /Resources/Media/Images/Icons/Files/cubemap.svg}" />
                                                                </DataTrigger>
                                                                <DataTrigger Binding="{Binding Extension}" Value="curveset">
                                                                    <Setter Property="Source" Value="{svgc:SvgImage /Resources/Media/Images/Icons/Files/curveset.svg}" />
                                                                </DataTrigger>
                                                                <DataTrigger Binding="{Binding Extension}" Value="dat">
                                                                    <Setter Property="Source" Value="{svgc:SvgImage /Resources/Media/Images/Icons/Files/dat.svg}" />
                                                                </DataTrigger>
                                                                <DataTrigger Binding="{Binding Extension}" Value="devices">
                                                                    <Setter Property="Source" Value="{svgc:SvgImage /Resources/Media/Images/Icons/Files/devices.svg}" />
                                                                </DataTrigger>
                                                                <DataTrigger Binding="{Binding Extension}" Value="dtex">
                                                                    <Setter Property="Source" Value="{svgc:SvgImage /Resources/Media/Images/Icons/Files/dtex.svg}" />
                                                                </DataTrigger>
                                                                <DataTrigger Binding="{Binding Extension}" Value="effect">
                                                                    <Setter Property="Source" Value="{svgc:SvgImage /Resources/Media/Images/Icons/Files/effect.svg}" />
                                                                </DataTrigger>
                                                                <DataTrigger Binding="{Binding Extension}" Value="envparam">
                                                                    <Setter Property="Source" Value="{svgc:SvgImage /Resources/Media/Images/Icons/Files/envparam.svg}" />
                                                                </DataTrigger>
                                                                <DataTrigger Binding="{Binding Extension}" Value="envprobe">
                                                                    <Setter Property="Source" Value="{svgc:SvgImage /Resources/Media/Images/Icons/Files/envprobe.svg}" />
                                                                </DataTrigger>
                                                                <DataTrigger Binding="{Binding Extension}" Value="es">
                                                                    <Setter Property="Source" Value="{svgc:SvgImage /Resources/Media/Images/Icons/Files/es.svg}" />
                                                                </DataTrigger>
                                                                <DataTrigger Binding="{Binding Extension}" Value="facialcustom">
                                                                    <Setter Property="Source" Value="{svgc:SvgImage /Resources/Media/Images/Icons/Files/facialcustom.svg}" />
                                                                </DataTrigger>
                                                                <DataTrigger Binding="{Binding Extension}" Value="facialsetup">
                                                                    <Setter Property="Source" Value="{svgc:SvgImage /Resources/Media/Images/Icons/Files/facialsetup.svg}" />
