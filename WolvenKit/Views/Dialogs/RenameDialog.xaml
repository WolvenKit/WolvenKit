<reactiveUi:ReactiveUserControl
    x:TypeArguments="dialogs:RenameDialogViewModel"
    x:Class="WolvenKit.Views.Dialogs.RenameDialog"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:reactiveUi="http://reactiveui.net"
    xmlns:hc="https://handyorg.github.io/handycontrol"
    xmlns:dialogs="clr-namespace:WolvenKit.ViewModels.Dialogs;assembly=WolvenKit.App">

    

    <Grid>
        <Grid.Resources>
            <ResourceDictionary>
                <ResourceDictionary.MergedDictionaries>
                    <hc:ThemeResources />
                    <hc:Theme />
                </ResourceDictionary.MergedDictionaries>
            </ResourceDictionary>
        </Grid.Resources>


        <hc:TextBox Width="400" VerticalAlignment="Top" />
        <Grid Margin="0,50,0,0">
            <Button
                Width="150"
                HorizontalAlignment="Right"
                Content="Confirm" />
            <Button
                Width="150"
                HorizontalAlignment="Left"
                Content="Cancel" />
        </Grid>
    </Grid>
</reactiveUi:ReactiveUserControl>
