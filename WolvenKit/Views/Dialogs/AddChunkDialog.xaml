<reactiveUi:ReactiveUserControl
    x:TypeArguments="dialogs:AddChunkDialogViewModel"
    x:Class="WolvenKit.Views.Dialogs.AddChunkDialog"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:reactiveUi="http://reactiveui.net"
    xmlns:hc="https://handyorg.github.io/handycontrol"
    xmlns:dialogs="clr-namespace:WolvenKit.ViewModels.Dialogs;assembly=WolvenKit.App">
    

    <Grid>

        <Grid.Resources>
            <ResourceDictionary>
                <ResourceDictionary.MergedDictionaries>
                    <hc:ThemeResources />
                    <hc:Theme />
                </ResourceDictionary.MergedDictionaries>
            </ResourceDictionary>
        </Grid.Resources>

        <Grid Margin="0,50,0,0">
            <Button
                Width="150"
                HorizontalAlignment="Right"
                Content="Confirm" />
            <Button
                Width="150"
                HorizontalAlignment="Left"
                Content="Cancel" />
        </Grid>
        <hc:ComboBox
            Width="380"
            Margin="0,0,0,0"
            VerticalAlignment="Top"
            hc:InfoElement.Title="Type"
            hc:InfoElement.TitlePlacement="Left"
            hc:InfoElement.TitleWidth="140"
            IsEditable="True"
            Text="None" />
    </Grid>
</reactiveUi:ReactiveUserControl>
