<reactiveUi:ReactiveUserControl
    x:Class="WolvenKit.Views.HomePage.Pages.GithubPageView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:hc="https://handyorg.github.io/handycontrol"
    xmlns:pages="clr-namespace:WolvenKit.ViewModels.HomePage.Pages;assembly=WolvenKit.App"
    xmlns:reactiveUi="http://reactiveui.net"
    xmlns:wv2="clr-namespace:Microsoft.Web.WebView2.Wpf;assembly=Microsoft.Web.WebView2.Wpf"
    x:TypeArguments="pages:GithubPageViewModel">

    <Grid
        MinWidth="300"
        MinHeight="300"
        hc:ThemeManager.RequestedAccentColor="{DynamicResource MahApps.Brushes.Accent3}">
        <Grid Margin="0,85,0,0">

            <wv2:WebView2 x:Name="cal" />
        </Grid>

        <StackPanel
            Height="85"
            Margin="25,0,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            Orientation="Horizontal">
            <TextBlock
                HorizontalAlignment="Center"
                VerticalAlignment="Center "
                FontSize="20"
                Text="GitHub" />
            <hc:Divider
                Height="45"
                Margin="15"
                HorizontalAlignment="Center"
                VerticalAlignment="Center"
                Orientation="Vertical" />
        </StackPanel>
    </Grid>
</reactiveUi:ReactiveUserControl>
