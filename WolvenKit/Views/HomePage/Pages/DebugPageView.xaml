                                <Image VerticalAlignment="Center" HorizontalAlignment="Center"  Source="{svgc:SvgImage Source=/Resources/Icons/FileIcons/app.svg}"/>

                                <TextBlock  Margin="5" Text="Okay"     />
                            </StackPanel>
                          
                        </TreeViewItem.Header>




                    </TreeViewItem>
                    <TreeViewItem>
