<UserControl
 x:Class="WolvenKit.Views.Editor.VisualEditor.Nodes.IN_Node"
 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
 xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
 xmlns:local="clr-namespace:WolvenKit.Views.Editor.VisualEditor.Nodes"
 xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
 xmlns:views="clr-namespace:NodeNetwork.Views;assembly=NodeNetwork"
 d:DesignHeight="450"
 d:DesignWidth="800"
 mc:Ignorable="d"
 >
    <views:NodeView
     x:Name="NodeView"
     ArrowSize="9"
     Background="DarkGreen"
     CornerRadius="5,35,35,5"
     Foreground="WhiteSmoke"
     TitleFontSize="14"
     >
        <views:NodeView.LeadingControlPresenterStyle>
            <Style TargetType="ContentPresenter">

                <Setter Property="ContentTemplate">
                    <Setter.Value>
                        <DataTemplate />
                    </Setter.Value>
                </Setter>
            </Style>
        </views:NodeView.LeadingControlPresenterStyle>
        <views:NodeView.TrailingControlPresenterStyle>
            <Style TargetType="ContentPresenter">
                <Setter Property="ContentTemplate">
                    <Setter.Value>
                        <DataTemplate />
                    </Setter.Value>
                </Setter>
            </Style>
        </views:NodeView.TrailingControlPresenterStyle>
    </views:NodeView>
</UserControl>
