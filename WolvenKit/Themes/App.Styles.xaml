<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:hc="https://handyorg.github.io/handycontrol"
    xmlns:helpers="clr-namespace:WolvenKit.Functionality.Helpers"
    xmlns:iconPacks="http://metro.mahapps.com/winfx/xaml/iconpacks"
    xmlns:o="http://schemas.microsoft.com/winfx/2006/xaml/presentation/options"
    xmlns:sfskin="clr-namespace:Syncfusion.SfSkinManager;assembly=Syncfusion.SfSkinManager.WPF"
    xmlns:svgc="http://sharpvectors.codeplex.com/svgc/"
    xmlns:syncfusion="http://schemas.syncfusion.com/wpf">

    <ResourceDictionary.MergedDictionaries>
        <sfskin:ResourceDictionaryProvider
            DictionaryName="Common"
            ThemeName="MaterialDark" />

        <sfskin:ResourceDictionaryProvider
            DictionaryName="Brushes"
            ThemeName="MaterialDark" />
        <ResourceDictionary Source="/Syncfusion.Themes.MaterialDark.WPF;component/DockingManager/DockingManager.xaml" />
        <ResourceDictionary Source="pack://application:,,,/HandyControl;component/Themes/basic/colors/dark.xaml" />
        <ResourceDictionary Source="pack://application:,,,/HandyControl;component/Themes/Theme.xaml" />
    </ResourceDictionary.MergedDictionaries>

    <!-- Colors -->
    <Color x:Key="WolvenKitRedColor">#DF2935</Color>
    <Color x:Key="WolvenKitRedShadowColor">#951C2D</Color>
    <Color x:Key="WolvenKitGreenColor">#28DE55</Color>
    <Color x:Key="WolvenKitGreenShadowColor">#1C9435</Color>
    <Color x:Key="WolvenKitCyanColor">#52F1FF</Color>
    <Color x:Key="WolvenKitCyanShadowColor">#4D8C9A</Color>
    <Color x:Key="WolvenKitYellowColor">#FFEA0A</Color>
    <Color x:Key="WolvenKitYellowShadowColor">#ABA108</Color>
    <Color x:Key="WolvenKitPurpleColor">CornflowerBlue</Color>
    <Color x:Key="WolvenKitDarkPinkColor">#AA336A</Color>
    <Color x:Key="WolvenKitTanColor">#D0A467</Color>

    <Color x:Key="WolvenKitCyanShadow50Color">#7F4D8C9A</Color>
    <Color x:Key="WolvenKitCyanShadow25Color">#203a40</Color>
    <Color x:Key="WolvenKitRedShadow50Color">#7F951C2D</Color>
    <Color x:Key="WolvenKitRedShadow25Color">#7F520e17</Color>
    <Color x:Key="WolvenKitYellowShadow50Color">#7FABA108</Color>
    <Color x:Key="WolvenKitYellowShadow25Color">#7F403c09</Color>

    <Color x:Key="WolvenKitGraphGridColor">#0DFFFFFF</Color>
    <Color x:Key="WolvenKitGraphAxisColor">Black</Color>
    <Color x:Key="WolvenKitGraphLabelColor">White</Color>
    <Color x:Key="WolvenKitGraphCurveColor">White</Color>

    <Color x:Key="WolvenKitLabelColor">White</Color>

    <!-- Brushes -->
    <SolidColorBrush x:Key="CustomSliderForegroundBrush" Color="#FF1E91EA" />
    <SolidColorBrush x:Key="CustomSliderBackgroundBrush" Color="#55ffffff" />
    <SolidColorBrush x:Key="CustomSliderHighlightBrush" Color="#FFEFEFEF" />

    <SolidColorBrush x:Key="WolvenKitSammyRed" Color="#DF2935" />
    <SolidColorBrush x:Key="WolvenKitSammyRedShaded" Color="#B71C34" />
    <SolidColorBrush x:Key="WolvenKitSammyBlack" Color="#292929" />

    <SolidColorBrush x:Key="WolvenKitRed" Color="{StaticResource WolvenKitRedColor}" />
    <SolidColorBrush x:Key="WolvenKitRedShadow" Color="{StaticResource WolvenKitRedShadowColor}" />
    <SolidColorBrush x:Key="WolvenKitGreen" Color="{StaticResource WolvenKitGreenColor}" />
    <SolidColorBrush x:Key="WolvenKitGreenShadow" Color="{StaticResource WolvenKitGreenShadowColor}" />
    <SolidColorBrush x:Key="WolvenKitCyan" Color="{StaticResource WolvenKitCyanColor}" />
    <SolidColorBrush x:Key="WolvenKitCyanShadow" Color="{StaticResource WolvenKitCyanShadowColor}" />
    <SolidColorBrush x:Key="WolvenKitYellow" Color="{StaticResource WolvenKitYellowColor}" />
    <SolidColorBrush x:Key="WolvenKitYellowShadow" Color="{StaticResource WolvenKitYellowShadowColor}" />
    <SolidColorBrush x:Key="WolvenKitPurple" Color="{StaticResource WolvenKitPurpleColor}" />
    <SolidColorBrush x:Key="WolvenKitDarkPink" Color="{StaticResource WolvenKitDarkPinkColor}" />
    <SolidColorBrush x:Key="WolvenKitTan" Color="{StaticResource WolvenKitTanColor}" />

    <SolidColorBrush x:Key="WolvenKitRed50" Color="#7FDF2935" />
    <SolidColorBrush x:Key="WolvenKitRedShadow50" Color="{StaticResource WolvenKitRedShadow50Color}" />
    <SolidColorBrush x:Key="WolvenKitCyan50" Color="#7F52F1FF" />
    <SolidColorBrush x:Key="WolvenKitCyanShadow50" Color="{StaticResource WolvenKitCyanShadow50Color}" />
    <SolidColorBrush x:Key="WolvenKitCyanShadow25" Color="{StaticResource WolvenKitCyanShadow25Color}" />
    <SolidColorBrush x:Key="WolvenKitYellow50" Color="#7FFFEA0A" />
    <SolidColorBrush x:Key="WolvenKitYellowShadow50" Color="{StaticResource WolvenKitYellowShadow50Color}" />
    <SolidColorBrush x:Key="WolvenKitPurple50" Color="#7F6495ED" />
    <SolidColorBrush x:Key="WolvenKitTan25" Color="#FFA7967D" />
    <SolidColorBrush x:Key="WolvenKitTan50" Color="#7FD0A467" />
    
    <SolidColorBrush x:Key="ForegroundColor" Color="#EBEBEB" />
    <SolidColorBrush x:Key="ForegroundColor_Grey_Light" Color="#cacaca" />
    <SolidColorBrush x:Key="ForegroundColor_Grey1" Color="#ababab" />
    <SolidColorBrush x:Key="ForegroundColor_Grey_Dark" Color="#969696" />
    <SolidColorBrush x:Key="ForegroundColor_Grey_Inactive" Color="#555555" />

    <!-- e.g. the hash box -->
    <SolidColorBrush x:Key="ForegroundColor_Control_Secondary" Color="#666666" />

    <SolidColorBrush x:Key="ForegroundColor_Cyan" Color="{StaticResource WolvenKitCyanColor}" />
    <SolidColorBrush x:Key="ForegroundColor_Yellow" Color="{StaticResource WolvenKitYellowColor}" />
    <SolidColorBrush x:Key="ForegroundColor_Red" Color="{StaticResource WolvenKitRedColor}" />

    <SolidColorBrush x:Key="BackgroundColor_Cyan" Color="{StaticResource WolvenKitCyanShadow50Color}" />
    <SolidColorBrush x:Key="BackgroundColor_Yellow" Color="{StaticResource WolvenKitYellowShadow50Color}" />
    <SolidColorBrush x:Key="BackgroundColor_Yellow_Dark" Color="{StaticResource WolvenKitYellowShadow25Color}" />
    <SolidColorBrush x:Key="BackgroundColor_Red" Color="{StaticResource WolvenKitRedShadow50Color}" />
    <SolidColorBrush x:Key="BackgroundColor_Red_Dark" Color="{StaticResource WolvenKitRedShadow25Color}" />

    <SolidColorBrush x:Key="BackgroundColor_Control" Color="#252525" />
    <SolidColorBrush x:Key="BackgroundColor_Frame_Active" Color="#313131" />
    <SolidColorBrush x:Key="BackgroundColor_Frame_Background" Color="#1F1F1F" />

    <SolidColorBrush x:Key="BackgroundColor_Button_Inactive" Color="#969696" />
    <SolidColorBrush x:Key="BackgroundColor_Button_Active" Color="#313131" />

    <!-- Graph and Curve Editors -->
    <SolidColorBrush x:Key="WolvenKitGraphGrid" Color="{StaticResource WolvenKitGraphGridColor}" />
    <SolidColorBrush x:Key="WolvenKitGraphAxis" Color="{StaticResource WolvenKitGraphAxisColor}" />
    <SolidColorBrush x:Key="WolvenKitGraphLabel" Color="{StaticResource WolvenKitGraphLabelColor}" />
    <SolidColorBrush x:Key="WolvenKitGraphCurve" Color="{StaticResource WolvenKitGraphCurveColor}" />

    <!-- Dialog Label styles -->
    <Style
        x:Key="WolvenkitDialogLabelStyle"
        TargetType="{x:Type Label}" BasedOn="{StaticResource  {x:Type Label}}">
        <Setter Property="Foreground" Value="{StaticResource WolvenKitLabelColor}"></Setter>
        <Setter Property="Width" Value="{StaticResource WolvenKitDialogLabelColumnWidth}"></Setter>
        <Setter Property="HorizontalAlignment" Value="Right"></Setter>
    </Style>

    <Style
        x:Key="WolvenkitDialogLabelStyleMedium"
        TargetType="{x:Type Label}" BasedOn="{StaticResource  WolvenkitDialogLabelStyle}">
        <Setter Property="Width" Value="{StaticResource WolvenKitDialogLabelColumnWidthMedium}"></Setter>
    </Style>

    <!-- Placeholder texts -->
    <Style x:Key="WolvenkitPlaceholderStyle" TargetType="{x:Type TextBlock}"
           BasedOn="{StaticResource {x:Type TextBlock}}">
        <Setter Property="Foreground" Value="{StaticResource ForegroundColor_Grey_Inactive}"></Setter>
        <Setter Property="VerticalAlignment" Value="Center"></Setter>
        <Setter Property="IsHitTestVisible" Value="False"></Setter>
        <Setter Property="Margin" Value="0"></Setter>
        <Setter Property="Padding" Value="10, 0"></Setter>
    </Style>
    
    <Style
        x:Key="WolvenKitTabControl"
        BasedOn="{StaticResource SyncfusionDocumentTabControlExtStyle}"
        TargetType="{x:Type syncfusion:TabControlExt}">
        <Setter Property="BorderThickness" Value="0" />
        <Setter Property="TabItemHoverBackground" Value="{StaticResource WolvenKitRedShadow}" />
        <Setter Property="TabItemHoverBorderBrush" Value="{StaticResource WolvenKitRedShadow}" />
        <Setter Property="TabItemSelectedBackground" Value="{StaticResource WolvenKitRed}" />
        <Setter Property="TabItemSelectedBorderBrush" Value="{StaticResource WolvenKitRedShadow}" />
        <Setter Property="TabItemSelectedForeground" Value="White" />
        <Setter Property="TabPanelBackground" Value="{StaticResource ContentBackgroundAlt2}" />

        <!-- Setter Property="TabPanelBackground" Value="#313131" / -->
    </Style>

    <!--
        Global style for nested syncfusion combobox, e.g. in ChunkMask editor.
        Doesn't work for base dropdown background yet
    -->
    <Style
        BasedOn="{StaticResource {x:Type syncfusion:ComboBoxAdv}}"
        TargetType="{x:Type syncfusion:ComboBoxAdv}">
        <Setter Property="Background" Value="#252525" />
    </Style>

    <!-- Give all textboxes the same background -->
    <Style
        x:Key="WolvenkitTextBoxStyle"
        BasedOn="{StaticResource {x:Type TextBox}}"
        TargetType="{x:Type TextBox}">
        <Setter Property="Background" Value="{StaticResource ContentBackgroundAlt2}" />
        <Setter Property="BorderBrush" Value="{StaticResource ContentBackgroundAlt3}" />
    </Style>

    <!-- Menu Bar Entry Style -->
    <Style
        x:Key="WolvenKitToolBarItem"
        TargetType="{x:Type MenuItem}">
        <Setter Property="Background" Value="{StaticResource BackgroundColor_Control}" />
        <Setter Property="Foreground" Value="{StaticResource ForegroundColor_Grey_Dark}" />
        <Setter Property="Margin" Value="3,4,3,0" />
        <Setter Property="Padding" Value="0,1,2,2" />

        <Style.Triggers>
            <DataTrigger
                Binding="{Binding Path=IsEnabled,
                                  RelativeSource={RelativeSource Self}}"
                Value="False">
                <Setter Property="Background" Value="{StaticResource BackgroundColor_Control}" />
                <Setter Property="Foreground" Value="{StaticResource ForegroundColor_Grey_Inactive}" />
            </DataTrigger>

            <DataTrigger
                Binding="{Binding Path=IsEnabled,
                                  RelativeSource={RelativeSource Self}}"
                Value="False">
                <Setter Property="Background" Value="{StaticResource BackgroundColor_Control}" />
                <Setter Property="Foreground" Value="{StaticResource ForegroundColor_Grey_Inactive}" />
            </DataTrigger>

            <DataTrigger
                Binding="{Binding Path=IsFocused,
                                  RelativeSource={RelativeSource Self}}"
                Value="False">
                <Setter Property="Background" Value="{StaticResource BackgroundColor_Frame_Active}" />
                <Setter Property="Foreground" Value="{StaticResource ForegroundColor_Grey_Light}" />
            </DataTrigger>

            <DataTrigger
                Binding="{Binding Path=IsMouseOver,
                                  RelativeSource={RelativeSource Self}}"
                Value="False">
                <Setter Property="Background" Value="{StaticResource BackgroundColor_Frame_Active}" />
                <Setter Property="Foreground" Value="{StaticResource ForegroundColor_Grey_Light}" />
            </DataTrigger>

            <DataTrigger
                Binding="{Binding Path=IsPressed,
                                  RelativeSource={RelativeSource Self}}"
                Value="False">
                <Setter Property="Background" Value="{StaticResource BackgroundColor_Frame_Background}" />
                <Setter Property="Foreground" Value="{StaticResource ForegroundColor}" />
            </DataTrigger>
        </Style.Triggers>
    </Style>

    <Style
        x:Key="ValidatingControlStyle"
        BasedOn="{StaticResource WolvenkitTextBoxStyle}"
        TargetType="{x:Type TextBox}">
        <Style.Triggers>
            <DataTrigger
                Binding="{Binding Path=IsValid}"
                Value="True">
                <Setter Property="BorderBrush" Value="{StaticResource ContentBackgroundAlt3}" />
            </DataTrigger>

            <DataTrigger
                Binding="{Binding Path=IsValid}"
                Value="False">
                <Setter Property="BorderBrush" Value="{DynamicResource BackgroundColor_Yellow}" />
            </DataTrigger>
        </Style.Triggers>

        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type TextBox}">
                    <Border
                        Name="border"
                        Background="{StaticResource ContentBackgroundAlt2}"
                        BorderBrush="{TemplateBinding BorderBrush}"
                        BorderThickness="1">
                        <ScrollViewer x:Name="PART_ContentHost" />
                    </Border>

                    <ControlTemplate.Triggers>
                        <MultiDataTrigger>
                            <MultiDataTrigger.Conditions>
                                <Condition Binding="{Binding RelativeSource={RelativeSource Self}, Path=IsFocused}" Value="True" />
                                <Condition Binding="{Binding Path=IsValid}" Value="False" />
                            </MultiDataTrigger.Conditions>

                            <Setter TargetName="border" Property="BorderBrush" Value="{DynamicResource BackgroundColor_Yellow}" />
                            <Setter TargetName="border" Property="BorderThickness" Value="2" />
                        </MultiDataTrigger>

                        <MultiDataTrigger>
                            <MultiDataTrigger.Conditions>
                                <Condition Binding="{Binding RelativeSource={RelativeSource Self}, Path=IsMouseOver}" Value="True" />
                                <Condition Binding="{Binding Path=IsValid}" Value="False" />
                            </MultiDataTrigger.Conditions>

                            <Setter TargetName="border" Property="BorderBrush" Value="{DynamicResource ForegroundColor_Yellow}" />
                            <Setter TargetName="border" Property="BorderThickness" Value="2" />
                        </MultiDataTrigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style
        x:Key="TextBoxTCSA"
        BasedOn="{StaticResource {x:Type TextBox}}"
        TargetType="{x:Type TextBox}">
        <Setter Property="helpers:TextBoxBehavior.TripleClickSelectAll" Value="True" />
    </Style>

    <DrawingBrush
        x:Key="CloudDrawingBrushMain"
        o:Freeze="True"
        Opacity="0.02"
        RenderOptions.CacheInvalidationThresholdMaximum="2.0"
        RenderOptions.CacheInvalidationThresholdMinimum="0.5"
        RenderOptions.CachingHint="Cache"
        Stretch="Uniform"
        TileMode="Tile"
        Viewport="0,0,90,90"
        ViewportUnits="Absolute">
        <DrawingBrush.Drawing>
            <ImageDrawing
                o:Freeze="True"
                ImageSource="pack://application:,,,/Resources/Media/Images/Application/Sammy_Grayscale.png"
                Rect="0,0,70,80" />
        </DrawingBrush.Drawing>
    </DrawingBrush>

    <LinearGradientBrush x:Key="ShadedBackgroundBrush" StartPoint="0,0" EndPoint="0,1">
        <GradientStopCollection>
            <GradientStop Offset="0.0" Color="#00000000" />
            <GradientStop Offset="0.1" Color="#44000000" />
            <GradientStop Offset="0.2" Color="#66000000" />
            <GradientStop Offset="0.4" Color="#D6000000" />
            <GradientStop Offset="0.4" Color="#D6000000" />
        </GradientStopCollection>
    </LinearGradientBrush>

    <!-- Position Slider Control -->
    <ControlTemplate
        x:Key="PositionSliderThumbTemplate"
        TargetType="{x:Type Thumb}">
        <Grid
            HorizontalAlignment="Center"
            VerticalAlignment="Center"
            UseLayoutRounding="True">
            <Path
                x:Name="grip"
                Margin="-1,-1"
                VerticalAlignment="Center"
                Data="M 0,0 A 180,180 180 1 1 1,1 Z"
                Fill="Transparent"
                Stretch="Fill"
                Stroke="{StaticResource CustomSliderForegroundBrush}"
                StrokeThickness="2.5"
                UseLayoutRounding="True" />
        </Grid>

        <ControlTemplate.Triggers>
            <Trigger Property="IsDragging" Value="true">
                <Setter TargetName="grip" Property="Fill" Value="{StaticResource CustomSliderForegroundBrush}" />
                <Setter TargetName="grip" Property="Stroke" Value="{StaticResource CustomSliderForegroundBrush}" />
            </Trigger>

            <Trigger Property="IsEnabled" Value="false">
                <Setter TargetName="grip" Property="Visibility" Value="Hidden" />
            </Trigger>

            <Trigger Property="IsKeyboardFocused" Value="true">
                <Setter TargetName="grip" Property="Fill" Value="{StaticResource CustomSliderForegroundBrush}" />
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

    <ControlTemplate
        x:Key="PositionSliderTemplate"
        TargetType="{x:Type Slider}">
        <Border
            x:Name="border"
            Background="Transparent"
            SnapsToDevicePixels="True">
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="{TemplateBinding MinHeight}" />
                </Grid.RowDefinitions>

                <DockPanel LastChildFill="True">
                    <Border
                        x:Name="PART_SelectionRange"
                        Height="3"
                        Visibility="Visible"
                        ClipToBounds="True">
                        <Rectangle
                            Margin="10,0,-10,0"
                            Fill="{StaticResource CustomSliderForegroundBrush}"
                            RadiusX="1"
                            RadiusY="1" />
                    </Border>

                    <Border
                        Height="3"
                        Visibility="Visible"
                        ClipToBounds="True">
                        <Rectangle
                            Margin="20,0,10,0"
                            Fill="{StaticResource CustomSliderBackgroundBrush}"
                            RadiusX="1"
                            RadiusY="1" />
                    </Border>
                </DockPanel>

                <Track x:Name="PART_Track">
                    <Track.Thumb>
                        <Thumb
                            x:Name="Thumb"
                            Width="{TemplateBinding MinHeight}"
                            Height="{TemplateBinding MinHeight}"
                            VerticalAlignment="Center"
                            Focusable="False"
                            OverridesDefaultStyle="True"
                            Template="{StaticResource PositionSliderThumbTemplate}" />
                    </Track.Thumb>
                </Track>
            </Grid>
        </Border>
    </ControlTemplate>

    <Style
        x:Key="PositionSliderStyle"
        TargetType="{x:Type Slider}">
        <Setter Property="AutoToolTipPlacement" Value="None" />
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="BorderBrush" Value="Transparent" />
        <Setter Property="Height" Value="20" />
        <Setter Property="IsMoveToPointEnabled" Value="True" />
        <Setter Property="MaxHeight" Value="20" />
        <Setter Property="MinHeight" Value="20" />
        <Setter Property="Minimum" Value="0" />
        <Setter Property="SelectionEnd" Value="{Binding Path=Value, RelativeSource={RelativeSource Self}}" />
        <Setter Property="SelectionStart" Value="0" />
        <Setter Property="Stylus.IsPressAndHoldEnabled" Value="false" />
        <Setter Property="Template" Value="{StaticResource PositionSliderTemplate}" />
        <Setter Property="VerticalAlignment" Value="Center" />
    </Style>

    <!-- Custom Slider Control -->
    <ControlTemplate
        x:Key="CustomSliderThumbTemplate"
        TargetType="{x:Type Thumb}">
        <Grid
            HorizontalAlignment="Center"
            VerticalAlignment="Center"
            UseLayoutRounding="True">
            <Rectangle
                x:Name="grip"
                Width="8"
                Height="{Binding Path=Height,
                                 RelativeSource={RelativeSource TemplatedParent}}"
                Fill="{StaticResource CustomSliderForegroundBrush}"
                RadiusX="3"
                RadiusY="3"
                Stretch="Fill"
                StrokeThickness="0"
                UseLayoutRounding="True" />
        </Grid>

        <ControlTemplate.Triggers>
            <Trigger Property="IsDragging" Value="true">
                <Setter TargetName="grip" Property="Fill" Value="{StaticResource CustomSliderHighlightBrush}" />
            </Trigger>

            <Trigger Property="IsEnabled" Value="false">
                <Setter TargetName="grip" Property="Fill" Value="{StaticResource CustomSliderBackgroundBrush}" />
            </Trigger>

            <Trigger Property="IsKeyboardFocused" Value="true">
                <Setter TargetName="grip" Property="Fill" Value="{StaticResource CustomSliderForegroundBrush}" />
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

    <ControlTemplate
        x:Key="CustomSliderTemplate"
        TargetType="{x:Type Slider}">
        <Border
            x:Name="border"
            Background="Transparent"
            SnapsToDevicePixels="True">
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="{TemplateBinding MinHeight}" />
                </Grid.RowDefinitions>

                <DockPanel LastChildFill="True">
                    <Border
                        x:Name="PART_SelectionRange"
                        Height="3"
                        Visibility="Visible"
                        ClipToBounds="True">
                        <Rectangle
                            Margin="4,0,-4,0"
                            Fill="{StaticResource CustomSliderForegroundBrush}"
                            RadiusX="1"
                            RadiusY="1" />
                    </Border>

                    <Border
                        Height="3"
                        Visibility="Visible"
                        ClipToBounds="True">
                        <Rectangle
                            x:Name="PART_NegativeSelection"
                            Margin="8,0,4,0"
                            Fill="{StaticResource CustomSliderBackgroundBrush}"
                            RadiusX="1"
                            RadiusY="1" />
                    </Border>
                </DockPanel>

                <Track x:Name="PART_Track">
                    <Track.Thumb>
                        <Thumb
                            x:Name="Thumb"
                            Width="8"
                            Height="{TemplateBinding MinHeight}"
                            VerticalAlignment="Center"
                            Focusable="False"
                            OverridesDefaultStyle="True"
                            Template="{StaticResource CustomSliderThumbTemplate}" />
                    </Track.Thumb>
                </Track>
            </Grid>
        </Border>

        <ControlTemplate.Triggers>
            <DataTrigger
                Binding="{Binding ElementName=Thumb,
                                  Path=IsDragging}"
                Value="True">
                <Setter TargetName="PART_NegativeSelection" Property="Fill" Value="{StaticResource CustomSliderHighlightBrush}" />
            </DataTrigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

    <Style
        x:Key="CustomSliderStyle"
        TargetType="{x:Type Slider}">
        <Setter Property="AutoToolTipPlacement" Value="None" />
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="BorderBrush" Value="Transparent" />
        <Setter Property="Height" Value="26" />
        <Setter Property="IsMoveToPointEnabled" Value="True" />
        <Setter Property="IsSnapToTickEnabled" Value="True" />
        <Setter Property="LargeChange" Value="0.01" />
        <Setter Property="MaxHeight" Value="26" />
        <Setter Property="MinHeight" Value="26" />
        <Setter Property="Minimum" Value="0" />
        <Setter Property="SelectionEnd" Value="{Binding Path=Value, RelativeSource={RelativeSource Self}}" />
        <Setter Property="SelectionStart" Value="{Binding Path=Minimum, RelativeSource={RelativeSource Self}}" />
        <Setter Property="SmallChange" Value="0.01" />
        <Setter Property="Stylus.IsPressAndHoldEnabled" Value="false" />
        <Setter Property="Template" Value="{StaticResource CustomSliderTemplate}" />
        <Setter Property="TickFrequency" Value="0.01" />
        <Setter Property="VerticalAlignment" Value="Center" />
    </Style>

    <Style
        x:Key="NetworkProgressBarStyle"
        TargetType="{x:Type ProgressBar}">
        <Setter Property="Background" Value="{StaticResource CustomSliderBackgroundBrush}" />
        <Setter Property="BorderThickness" Value="0" />
        <Setter Property="Height" Value="3" />
        <Setter Property="Margin" Value="10,0" />
        <Setter Property="Maximum" Value="1" />
        <Setter Property="Minimum" Value="0" />
    </Style>

    <Style
        x:Key="ModernToggleButtonStyle"
        TargetType="ToggleButton">
        <Style.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="Background" Value="#22FFFFFF" />
            </Trigger>

            <Trigger Property="IsPressed" Value="True">
                <Setter Property="Background" Value="#44FFFFFF" />
            </Trigger>

            <Trigger Property="IsChecked" Value="True">
                <Setter Property="Background" Value="#44FFFFFF" />
            </Trigger>
        </Style.Triggers>

        <Setter Property="Background" Value="Transparent" />
        <Setter Property="BorderThickness" Value="0" />
        <Setter Property="Foreground" Value="{StaticResource CustomSliderHighlightBrush}" />
        <Setter Property="Height" Value="45" />
        <Setter Property="Padding" Value="12" />

        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type ToggleButton}">
                    <Border
                        Padding="{TemplateBinding Padding}"
                        Background="{TemplateBinding Background}"
                        BorderThickness="0">
                        <ContentPresenter
                            x:Name="contentPresenter"
                            HorizontalAlignment="Center"
                            VerticalAlignment="Center" />
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="Width" Value="45" />
    </Style>

    <Style
        x:Key="ModernButtonStyle"
        TargetType="{x:Type Button}">
        <Style.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="Background" Value="#22FFFFFF" />
                <Setter Property="Foreground" Value="{StaticResource CustomSliderHighlightBrush}" />
            </Trigger>

            <Trigger Property="IsPressed" Value="True">
                <Setter Property="Background" Value="#44FFFFFF" />
                <Setter Property="Foreground" Value="{StaticResource CustomSliderHighlightBrush}" />
            </Trigger>
        </Style.Triggers>

        <Setter Property="Background" Value="Transparent" />
        <Setter Property="BorderThickness" Value="0" />
        <Setter Property="Foreground" Value="{StaticResource CustomSliderHighlightBrush}" />
        <Setter Property="Height" Value="45" />
        <Setter Property="Padding" Value="12" />

        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type Button}">
                    <Border
                        Padding="{TemplateBinding Padding}"
                        Background="{TemplateBinding Background}"
                        BorderThickness="0">
                        <ContentPresenter
                            x:Name="contentPresenter"
                            HorizontalAlignment="Center"
                            VerticalAlignment="Center" />
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="Width" Value="45" />
    </Style>

    <Style
        x:Key="ControlsGridStyle"
        TargetType="{x:Type Grid}">
        <Setter Property="Background" Value="{DynamicResource ShadedBackgroundBrush}" />
        <Setter Property="Height" Value="250" />
        <Setter Property="VerticalAlignment" Value="Bottom" />
    </Style>

    <!-- Custom Scrollbar Control -->
    <!-- Scrollbar Thumbs -->
    <Style
        x:Key="ScrollThumbs"
        TargetType="{x:Type Thumb}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type Thumb}">
                    <Grid x:Name="Grid">
                        <Rectangle
                            Width="Auto"
                            Height="Auto"
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Stretch"
                            Fill="Transparent" />

                        <Border
                            x:Name="Rectangle1"
                            Width="Auto"
                            Height="Auto"
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Stretch"
                            Background="{TemplateBinding Background}"
                            CornerRadius="5" />
                    </Grid>

                    <ControlTemplate.Triggers>
                        <Trigger Property="Tag" Value="Horizontal">
                            <Setter TargetName="Rectangle1" Property="Height" Value="7" />
                            <Setter TargetName="Rectangle1" Property="Width" Value="Auto" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <!-- ScrollBars -->
    <Style
        x:Key="{x:Type ScrollBar}"
        TargetType="{x:Type ScrollBar}">
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="Foreground" Value="#8c8c8c" />
        <Setter Property="Stylus.IsFlicksEnabled" Value="false" />

        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type ScrollBar}">
                    <Grid
                        x:Name="GridRoot"
                        Width="8"
                        Background="{TemplateBinding Background}">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="0.00001*" />
                        </Grid.RowDefinitions>

                        <Track
                            x:Name="PART_Track"
                            Grid.Row="0"
                            Focusable="false"
                            IsDirectionReversed="true">
                            <Track.Thumb>
                                <Thumb
                                    x:Name="Thumb"
                                    Background="{TemplateBinding Foreground}"
                                    Style="{DynamicResource ScrollThumbs}" />
                            </Track.Thumb>

                            <Track.IncreaseRepeatButton>
                                <RepeatButton
                                    x:Name="PageUp"
                                    Command="ScrollBar.PageDownCommand"
                                    Focusable="false"
                                    Opacity="0" />
                            </Track.IncreaseRepeatButton>

                            <Track.DecreaseRepeatButton>
                                <RepeatButton
                                    x:Name="PageDown"
                                    Command="ScrollBar.PageUpCommand"
                                    Focusable="false"
                                    Opacity="0" />
                            </Track.DecreaseRepeatButton>
                        </Track>
                    </Grid>

                    <ControlTemplate.Triggers>
                        <Trigger SourceName="Thumb" Property="IsMouseOver" Value="true">
                            <Setter TargetName="Thumb" Property="Background" Value="{DynamicResource ButtonSelectBrush}" />
                        </Trigger>

                        <Trigger SourceName="Thumb" Property="IsDragging" Value="true">
                            <Setter TargetName="Thumb" Property="Background" Value="{DynamicResource DarkBrush}" />
                        </Trigger>

                        <Trigger Property="IsEnabled" Value="false">
                            <Setter TargetName="Thumb" Property="Visibility" Value="Collapsed" />
                        </Trigger>

                        <Trigger Property="Orientation" Value="Horizontal">
                            <Setter Property="Height" Value="8" />
                            <Setter Property="Width" Value="Auto" />

                            <Setter TargetName="GridRoot" Property="LayoutTransform">
                                <Setter.Value>
                                    <RotateTransform Angle="-90" />
                                </Setter.Value>
                            </Setter>

                            <Setter TargetName="PART_Track" Property="LayoutTransform">
                                <Setter.Value>
                                    <RotateTransform Angle="-90" />
                                </Setter.Value>
                            </Setter>

                            <Setter TargetName="PageDown" Property="Command" Value="ScrollBar.PageLeftCommand" />
                            <Setter TargetName="PageUp" Property="Command" Value="ScrollBar.PageRightCommand" />
                            <Setter TargetName="Thumb" Property="Tag" Value="Horizontal" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="Width" Value="8" />
    </Style>

