    <Style
        x:Key="FileIconIconStyle"
        TargetType="{x:Type iconPacks:PackIconControl}">
        <Setter Property="Visibility" Value="Visible" />
        <Style.Triggers>
            <DataTrigger
                Binding="{Binding Icon,
                                  RelativeSource={RelativeSource AncestorType=UserControl}}"
                Value="archive">
                <Setter Property="Foreground" Value="{StaticResource WolvenKitRed}" />
                <Setter Property="Kind" Value="{x:Static iconPacks:PackIconForkAwesomeKind.Archive}" />
            </DataTrigger>
            <DataTrigger
                Binding="{Binding FullIcon,
                                  RelativeSource={RelativeSource AncestorType=UserControl}}"
                Value="ModDirectoryTop">
                <Setter Property="Foreground" Value="{StaticResource WolvenKitRed}" />
                <Setter Property="Kind" Value="{x:Static iconPacks:PackIconMaterialKind.Folder}" />
            </DataTrigger>
            <DataTrigger
                Binding="{Binding FullIcon,
                                  RelativeSource={RelativeSource AncestorType=UserControl}}"
                Value="ModDirectoryTopOpen">
                <Setter Property="Foreground" Value="{StaticResource WolvenKitRed}" />
                <Setter Property="Kind" Value="{x:Static iconPacks:PackIconMaterialKind.FolderOpen}" />
            </DataTrigger>
            <DataTrigger
                Binding="{Binding FullIcon,
                                  RelativeSource={RelativeSource AncestorType=UserControl}}"
                Value="RawDirectoryTop">
                <Setter Property="Foreground" Value="{StaticResource WolvenKitTan}" />
                <Setter Property="Kind" Value="{x:Static iconPacks:PackIconMaterialKind.Folder}" />
            </DataTrigger>
            <DataTrigger
                Binding="{Binding FullIcon,
                                  RelativeSource={RelativeSource AncestorType=UserControl}}"
                Value="RawDirectoryTopOpen">
                <Setter Property="Foreground" Value="{StaticResource WolvenKitTan}" />
                <Setter Property="Kind" Value="{x:Static iconPacks:PackIconMaterialKind.FolderOpen}" />
            </DataTrigger>

            <DataTrigger
                Binding="{Binding FullIcon,
                                  RelativeSource={RelativeSource AncestorType=UserControl}}"
                Value="ResourceDirectoryTop">
                <Setter Property="Foreground" Value="{StaticResource WolvenKitCyan}" />
                <Setter Property="Kind" Value="{x:Static iconPacks:PackIconMaterialKind.Folder}" />
            </DataTrigger>
            <DataTrigger
                Binding="{Binding FullIcon,
                                  RelativeSource={RelativeSource AncestorType=UserControl}}"
                Value="ResourceDirectoryTopOpen">
                <Setter Property="Foreground" Value="{StaticResource WolvenKitCyan}" />
                <Setter Property="Kind" Value="{x:Static iconPacks:PackIconMaterialKind.FolderOpen}" />
            </DataTrigger>

            <DataTrigger
                Binding="{Binding FullIcon,
                                  RelativeSource={RelativeSource AncestorType=UserControl}}"
                Value="ClosedDirImageKey">
                <Setter Property="Foreground" Value="{StaticResource WolvenKitTan}" />
                <Setter Property="Kind" Value="{x:Static iconPacks:PackIconMaterialKind.Folder}" />
            </DataTrigger>
            <DataTrigger
                Binding="{Binding FullIcon,
                                  RelativeSource={RelativeSource AncestorType=UserControl}}"
                Value="OpenDirImageKey">
                <Setter Property="Foreground" Value="{StaticResource WolvenKitTan}" />
                <Setter Property="Kind" Value="{x:Static iconPacks:PackIconMaterialKind.FolderOpen}" />
            </DataTrigger>
            <DataTrigger
                Binding="{Binding FullIcon,
                                  RelativeSource={RelativeSource AncestorType=UserControl}}"
                Value="OpenDirImageKeyOpen">
                <Setter Property="Foreground" Value="{StaticResource WolvenKitTan}" />
                <Setter Property="Kind" Value="{x:Static iconPacks:PackIconMaterialKind.FolderOpen}" />
            </DataTrigger>
            <Trigger Property="Kind" Value="{x:Null}">
                <Setter Property="Visibility" Value="Collapsed" />
            </Trigger>
        </Style.Triggers>
    </Style>

    <Style
        x:Key="SearchBarButtonStyle"
        BasedOn="{StaticResource {x:Type Button}}"
        TargetType="Button">
        <Setter Property="BorderBrush" Value="{StaticResource ForegroundColor_Grey_Dark}" />
        <Setter Property="Foreground" Value="{StaticResource ForegroundColor_Grey_Dark}" />

        <Style.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="Foreground" Value="{StaticResource ForegroundColor_Grey_Light}" />
            </Trigger>

            <Trigger Property="IsKeyboardFocused" Value="True">
                <Setter Property="Foreground" Value="{StaticResource ForegroundColor_Grey_Light}" />
            </Trigger>

            <Trigger Property="IsFocused" Value="True">
                <Setter Property="Foreground" Value="{StaticResource ForegroundColor_Grey_Light}" />
            </Trigger>

            <DataTrigger
                Binding="{Binding Path=(Validation.HasError),
                                  RelativeSource={RelativeSource Self}}"
                Value="True">
                <Setter Property="Foreground" Value="{StaticResource ForegroundColor_Grey_Light}" />
            </DataTrigger>
        </Style.Triggers>
    </Style>

    <Style
        x:Key="SearchBarStyle"
        BasedOn="{StaticResource SearchBarPlus}"
        TargetType="{x:Type hc:SearchBar}">
        <Setter Property="Background" Value="{StaticResource BackgroundColor_Control}" />
        <Setter Property="BorderBrush" Value="{StaticResource BackgroundColor_Frame_Active}" />
        <Setter Property="FontSize" Value="{DynamicResource WolvenKitFontSubTitle}" />

        <Style.Resources>
            <Style
                BasedOn="{StaticResource SearchBarButtonStyle}"
                TargetType="Button" />
        </Style.Resources>
    </Style>
